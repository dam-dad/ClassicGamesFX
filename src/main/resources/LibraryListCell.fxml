<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane fx:id="view" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <Label fx:id="gameName" text="label" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="playbtn" disable="true" mnemonicParsing="false" text="Jugar" />
            <Button fx:id="DeleteButton" mnemonicParsing="false" onAction="#OnDeleteAction" text="Eliminar" />
         </children>
      </HBox>
   </right>
</BorderPane>
